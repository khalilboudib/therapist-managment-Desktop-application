<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: FFFFF;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui_tp.tp_poo.StartEpreuve">
   <children>
      <Text layoutX="200.0" layoutY="70.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Question libre">
         <font>
            <Font name="Poppins Bold" size="28.0" />
         </font>
      </Text>
      <TextField fx:id="QueLibreReponse" layoutX="195.0" layoutY="147.0" prefHeight="80.0" prefWidth="210.0" promptText="reponse" styleClass="styled-inputfield" stylesheets="@styles.css" />
      <TextField fx:id="QueLibreScore" layoutX="340.0" layoutY="261.0" prefHeight="28.0" prefWidth="65.0" promptText="score" styleClass="styled-inputfield" stylesheets="@styles.css" />
      <Button layoutX="354.0" layoutY="310.0" mnemonicParsing="false" onAction="#nextQueLibre" prefHeight="25.0" prefWidth="51.0" styleClass="button-addchoice" stylesheets="@styles.css" text="next" />
      <Label fx:id="QueLibreEnonce" layoutX="286.0" layoutY="99.0" textAlignment="CENTER" />
      <Button layoutX="195.0" layoutY="310.0" mnemonicParsing="false" onAction="#loadQueLibre" prefHeight="25.0" prefWidth="51.0" styleClass="button-addchoice" stylesheets="@styles.css" text="load" />
      <Text layoutX="200.0" layoutY="280.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Score(?/10)  : ">
         <font>
            <Font name="Poppins Bold" size="14.0" />
         </font>
      </Text>
   </children>
</AnchorPane>

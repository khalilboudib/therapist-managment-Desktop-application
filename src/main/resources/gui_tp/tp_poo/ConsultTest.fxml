                <Insets bottom="20" left="20" right="20" top="20" />
            </padding>
            <children>
                <ImageView fx:id="profileImage1" fitHeight="51.0" fitWidth="153.0">
                    <image>
                        <Image url="@logo.png" />
                    </image>
                </ImageView>
                <HBox prefHeight="57.0" prefWidth="160.0">
                    <children>
                        <ImageView fx:id="profileImage" fitHeight="51.0" fitWidth="51.0">
                            <image>
                                <Image url="@profile.png" />
                            </image>
                        </ImageView>
                        <Label text="MAACHI">
                            <font>
                                <Font name="GothamPro-Bold" size="18.0" />
                            </font>
                            <HBox.margin>
                                <Insets left="10.0" top="15.0" />
                            </HBox.margin>
                        </Label>
                    </children>
                    <VBox.margin>
                        <Insets top="20.0" />
                    </VBox.margin>
                </HBox>
                <Button alignment="CENTER" onAction="#GoHome" prefHeight="30.0" prefWidth="164.0" styleClass="button-transparent" stylesheets="@styles.css" text="  Home                 ">
                    <graphic>
                        <ImageView fx:id="profileImage2" fitHeight="26.0" fitWidth="23.0" preserveRatio="true">
                            <image>
                                <Image url="@home.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button onAction="#GoPatient" prefHeight="30.0" prefWidth="164.0" styleClass="button-transparent" stylesheets="@styles.css" text=" Clients               ">
                    <graphic>
                        <ImageView fx:id="profileImage21" fitHeight="26.0" fitWidth="23.0" preserveRatio="true">
                            <image>
                                <Image url="@client.png" />
                            </image>
                        </ImageView>
                    </graphic>
                    <font>
                        <Font name="GothamPro-Medium" size="12.0" />
                    </font>
                </Button>
                <Button onAction="#GoRendezVous" prefHeight="30.0" prefWidth="164.0" styleClass="button-transparent" stylesheets="@styles.css" text=" Rendez-Vous      ">

                        <ImageView fx:id="profileImage211" fitHeight="26.0" fitWidth="23.0" preserveRatio="true">
                            <image>
                                <Image url="@rendezvous.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button onAction="#GoTest" prefHeight="30.0" prefWidth="164.0" styleClass="button-transparent" stylesheets="@styles.css" text=" Tests                   ">
                    <graphic>
                        <ImageView fx:id="profileImage212" fitHeight="26.0" fitWidth="23.0" preserveRatio="true">
                            <image>
                                <Image url="@tests.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button onAction="#GoAnamnese" prefHeight="30.0" prefWidth="164.0" styleClass="button-transparent" stylesheets="@styles.css" text=" AnamnÃ¨se          ">
                    <graphic>
                        <ImageView fx:id="profileImage213" fitHeight="26.0" fitWidth="23.0" preserveRatio="true">
                            <image>
                                <Image url="@anamnese.png" />
                            </image>
                        </ImageView>
                    </graphic>
                </Button>
                <Button onAction="#LogOut" prefHeight="30.0" prefWidth="164.0" styleClass="button-transparent" stylesheets="@styles.css" text=" Log Out        ">
                    <graphic>
                        <ImageView fx:id="profileImage2131" fitHeight="26.0" fitWidth="23.0" preserveRatio="true">
                            <image>
                                <Image url="@logout.png" />
                            </image>
                        </ImageView>

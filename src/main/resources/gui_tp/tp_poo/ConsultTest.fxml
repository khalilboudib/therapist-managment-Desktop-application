                    <children>
                        <VBox alignment="TOP_CENTER" prefHeight="288.0" prefWidth="707.0" spacing="10.0" style="-fx-background-color: white; -fx-background-radius: 20;">
                            <children>
                                <TableView fx:id="AnamTab" prefHeight="251.0" prefWidth="686.0" stylesheets="@styles.css">
                                    <columns>
                                        <TableColumn minWidth="0.0" prefWidth="0.0" text="Column X" />
                                        <TableColumn fx:id="QueColm" prefWidth="185.0" text="Nom Test" />
                                        <TableColumn fx:id="ReponseColm" prefWidth="411.0" text="Conclusion" />
                              <TableColumn fx:id="ScoreColm" prefWidth="89.0" text="Score" />
                                    </columns>
                                </TableView>
                        <Label text="Observation : " />
                        <Label fx:id="ObservLabel" />
                            </children>
                            <padding>
                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                            </padding>
                        </VBox>
                    </children>
                </HBox>
                <HBox prefHeight="18.0" prefWidth="717.0" spacing="250.0">
                    <padding>
                        <Insets bottom="15.0" />
                    </padding>
                    <children>
                        <Button mnemonicParsing="false" onAction="#goback" prefHeight="25.0" prefWidth="80.0" styleClass="button-addchoice" stylesheets="@styles.css" text="Back">
                            <font>
                                <Font size="11.0" />
                            </font>
                        </Button>
                  <Button mnemonicParsing="false" onAction="#load" prefHeight="25.0" prefWidth="80.0" styleClass="button-addchoice" stylesheets="@styles.css" text="Load">
                     <font>
                        <Font size="11.0" />
                     </font>
                  </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </center>

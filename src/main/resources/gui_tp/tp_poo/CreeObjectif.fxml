<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui_tp.tp_poo.StartFiche">
    <children>
      <VBox alignment="TOP_CENTER" layoutX="208.0" layoutY="123.0" prefHeight="266.0" prefWidth="302.0" spacing="10.0" style="-fx-background-color: WHITE; -fx-background-radius: 20;">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Creation de l'objectif">
               <font>
                  <Font name="GothamPro" size="12.0" />
               </font>
            </Text>
            <VBox maxWidth="250.0" prefHeight="46.0" prefWidth="250.0" spacing="2.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Nom">
                     <font>
                        <Font name="Gotham-Light" size="12.0" />
                     </font>
                  </Text>
                  <TextField fx:id="NomObjectif" prefHeight="56.0" prefWidth="250.0" promptText="nom" styleClass="styled-inputfield" stylesheets="@styles.css" />
               </children>
            </VBox>
            <VBox maxWidth="250.0" prefWidth="522.0" spacing="2.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Note">
                     <font>
                        <Font name="Gotham-Light" size="12.0" />
                     </font>
                  </Text>
                    <TextField fx:id="NoteObjec" prefHeight="38.0" promptText="note" styleClass="styled-inputfield" stylesheets="@styles.css" />
               </children>
            </VBox>
            <VBox maxWidth="250.0" prefWidth="522.0" spacing="2.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Type de l'objectif">
                     <font>
                        <Font name="Gotham-Light" size="12.0" />
                     </font>
                  </Text>
                  <ChoiceBox fx:id="TypeObjec" prefHeight="36.0" prefWidth="248.0" styleClass="styled-choicebox" stylesheets="@styles.css" />
               </children>
            </VBox>
              <Button mnemonicParsing="false" onAction="#AddObjectif" prefHeight="33.0" prefWidth="142.0" styleClass="button-addchoice" stylesheets="@styles.css" text="Ajouter" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
    </children>
</AnchorPane>

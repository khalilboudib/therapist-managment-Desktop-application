<BorderPane maxHeight="768.0" maxWidth="1366.0" stylesheets="@table.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui_tp.tp_poo.detailpatient">
